@server

port: 8080
servername: localhost
maxbodysize: 5000000
error: www/

route: /
.method: GET
.root: www
.index: index.html

route: /autoindex
.method: DELETE
.root: www/autoindex/
.autoindex: on
.index: autoindex.html

route: /post_body
.method: POST
.maxbodysize: 100

