{
	"index": "index.html",
	"allowed_methods": "GET POST",
	"python_cgi_extension": "py",
	"perl_cgi_extension": "pl",
	"upload_root": "./www/cgi/upload",
	"root": "./www/",
	"uploadable": false,
	"autoindex": false,
	"route index": {
		"path": "/"
	},
	"route post": {
		"uploadable": true,
		"path": "/post",
		"index": "post.html"
	},
	"route autoindex": {
		"path": "/autoindex",
		"method": "GET POST DELETE",
		"autoindex": true,
		"index": "autoindex.html",
		"root": "./www/autoindex"
	},
	"route bonus": {
		"path": "/bonus",
		"index": "bonus.html"
	},
	"code 404": {
		"code": 404,
		"redirect": "/404"
	},
	"route 404": {
		"index": "error_404.html",
		"autoindex": true,
		"path": "/404",
		"root": "./www/error_pages"
	},
	"server0": {
		"host": "localhost",
		"port": 8080
	},
	"server1": {
		"host": "localhost",
		"port": 8090
	}
}