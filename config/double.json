{
	"index": "index.html",
	"allowed_methods": "GET POST",
	"python_cgi_extension": "py",
	"perl_cgi_extension": "pl",
	"upload_root": "./media",
	"root": "./www/",
	"uploadable": false,
	"autoindex": false,
	"route index": {
		"path": "/"
	},
	"route post": {
		"path": "/post",
		"index": "post.html"
	},
	"route autoindex": {
		"path": "/post",
		"autoindex": true,
		"index": "autoindex.html",
		"root": "./www/autoindex"
	},
	"route bonus": {
		"path": "/post",
		"index": "bonus.html"
	},
	"server0": {
		"host": "localhost",
		"port": 8080,
		"server_name": null, 
		"code 404": {
			"code": 404,
			"redirect": "/404"
		},
		"code 401": {
			"code": 401,
			"redirect": "/404",
			"response_code": 301
		},
		"route 404": {
			"index": "error_404.html",
			"autoindex": true,
			"path": "/404",
			"root": "./www/error_pages"
		}
	},
	"server1": {
		"host": "localhost",
		"port": 8090,
		"server_name": null, 
		"code 404": {
			"code": 404,
			"redirect": "/404"
		},
		"code 401": {
			"code": 401,
			"redirect": "/404",
			"response_code": 301
		},
		"route 404": {
			"index": "error_404.html",
			"autoindex": true,
			"path": "/404",
			"root": "./www/error_pages"
		}
	}
}